#include <iostream>

using namespace std;

int main()
{
    int n,maxi=-1,uc,y,i;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        cin>>y;
        
        while(y>0)
        {
            uc=y%10;
            if(maxi<uc)
            {
                maxi=uc; 
            }
            y=y/10;
        }
        cout<<maxi;
        maxi=0;
    }
    return 0;
}
